{
  "name": "boilerplate-fullstack",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "engines": {
    "node": "16.14.2"
  },
  "scripts": {
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server dev:css",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server server/index.js",
    "build": "run-p build:client build:server",
    "build:client": "vite build",
    "build:server": "nodemon --watch server server/index.js",
    "lint": "eslint --ext .js,.jsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "dev:css": "tailwindcss -i server/public/main.css -o server/public/output.css --watch",
    "vite:start": "vite",
    "vite:build": "vite build",
    "test:e2e": "jest --selectProjects e2e --runInBand"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@react-three/drei": "^9.17.1",
    "@react-three/fiber": "^8.1.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "autoprefixer": "^10.4.7",
    "camera-controls": "^1.35.0",
    "dotenv": "^16.0.2",
    "esbuild": "^0.14.49",
    "express": "^4.17.2",
    "knex": "^2.1.0",
    "pg": "^8.7.1",
    "playwright": "^1.23.4",
    "postcss": "^8.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.2.2",
    "redux": "^4.2.0",
    "sqlite3": "^5.0.8",
    "superagent": "7.1.1",
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@playwright/test": "^1.23.4",
    "@react-three/test-renderer": "^9.0.0",
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.1.6",
    "vite": "^2.9.13",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
